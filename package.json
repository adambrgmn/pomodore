    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.4",
    "clean-webpack-plugin": "^0.1.8",
    "css-loader": "^0.23.1",
    "eslint": "^2.7.0",
