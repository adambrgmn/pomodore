{
  "name": "pomodore",
  "version": "2.1.10",
  "description": "A small Pomodore-timer",
  "main": "server.js",
  "scripts": {
    "start": "export NODE_ENV=development && node server",
    "preview": "export NODE_ENV=production && npm run build && node server",
    "build": "export NODE_ENV=production && rimraf dist && webpack --progress --profile --colors",
    "build:stats": "webpack --profile --json > stats.json",
    "test": "mocha --compilers js:babel-core/register --require ./test/setup.js 'app/**/*-spec.@(js|jsx)'",
    "test:watch": "npm run test -- --watch --watch-extensions jsx",
    "deploy": "pm2 deploy ecosystem.json production",
    "deploy:setup": "pm2 deploy ecosystem.json production setup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adambrgmn/pomodore.git"
  },
  "bugs": {
    "url": "https://github.com/adambrgmn/pomodore/issues"
  },
  "author": "Adam Bergman",
  "homepage": {
    "url": "http://www.fransvilhelm.com/"
  },
  "keywords": [
    "Express",
    "React",
    "ES2015",
    "ES6",
    "Webpack",
    "Redux",
    "Eslint"
  ],
  "license": "MIT",
